class Solution {
    public int countStudents(int[] students, int[] sandwiches) {
        Stack<Integer> st = new Stack<>();
        Queue<Integer> q = new LinkedList<>();
        int n = students.length;
        for (int i = 0; i < n; i++) {
            st.push(sandwiches[n - i - 1]);
            q.add(students[i]);
        }
        int back = 0;
        while (st.size() > 0 && q.size() > 0) {
            if (back == q.size())
                return back;
            if (q.peek() == st.peek()) {
                q.remove();
                st.pop();
                back = 0;
            } else {
                int x = q.remove();
                q.add(x);
                back++;
            }
        }
        return 0;
    }
}
