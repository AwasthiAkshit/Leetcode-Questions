class Solution {
    private int trib(int n,int[] dp){
        if(n<=1) return n;
        if(n==2) return 1;
        if(dp[n]!=-1) return dp[n];
        dp[n] = trib(n-1,dp)+trib(n-2,dp)+trib(n-3,dp);
        return dp[n];

    }
    public int tribonacci(int n) {
        int[] dp = new int[n+1];
        for(int i=0;i<n+1;i++){
            dp[i] = -1;
        }
        return trib(n,dp);

    }
}
