class Solution {
    public int[] findMissingAndRepeatedValues(int[][] grid) {
        int[] arr = new int[2];
        HashSet<Integer> set = new HashSet<>();
        int n = grid.length;
        int sum = 0;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                if(set.contains(grid[i][j])){
                    arr[0] = grid[i][j];
                }
                else{
                    set.add(grid[i][j]);
                    sum = sum+grid[i][j];
                }
                

            }
        }
        int total = 0;
      for(int i=1;i<=n*n;i++){
        total = total+i;

      }
      arr[1] = total-sum;
      return arr;

    }
}
