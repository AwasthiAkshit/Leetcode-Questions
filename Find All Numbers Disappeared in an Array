class Solution {
    public List<Integer> findDisappearedNumbers(int[] nums) {
        List<Integer> arr = new ArrayList<>();
        int n = nums.length;
        int[] temp = new int[n];
        for (int i = 0; i < n; i++) {
            temp[i] = i + 1;
        }
        for (int i = 0; i < n; i++) {
            temp[nums[i] - 1] = 0;
        }
        for (int i = 0; i < n; i++) {
            if (temp[i] != 0)
                arr.add(temp[i]);
        }
        return arr;
    }
}
