class Solution {
    public boolean threeConsecutiveOdds(int[] arr) {
        int n = arr.length;
        for(int i=0;i<n-2;i++){
            int f = arr[i];
            int j = arr[i+1];
            int k = arr[i+2];
            if(f%2!=0&&j%2!=0&&k%2!=0) return true;
        }
        return false;
    }
}
