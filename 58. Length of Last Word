class Solution {
    public int lengthOfLastWord(String s) {
        int count = 0;
        if (s.length() == 1)
            return 1;
        s = s.trim();
        int i = s.length() - 1;
        while (i >= 0 && s.charAt(i) != ' ') {
            count++;
            i--;
        }
        return count;
    }
}
