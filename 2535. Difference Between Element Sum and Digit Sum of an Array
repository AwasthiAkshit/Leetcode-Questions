class Solution {
    public int differenceOfSum(int[] nums) {
        int n = nums.length;
        int eleSum = 0;
        int digSum = 0;
        for(int i=0;i<n;i++){
            eleSum = eleSum+nums[i];
            int num = nums[i];
            int sum = 0;
            while(num>0){
                int dig = num%10;
                sum = sum+dig;
                num = num/10;

            }
            digSum = digSum+sum;

        }
        return eleSum-digSum;
    }
}
